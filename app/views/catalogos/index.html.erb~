<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Suporte</th>
      <th>Localização</th>
      <th>Nome da Aquisição</th>
      <th>Data de Aquisição</th>
      <th>Fibra do Tecido</th>
      <th>Codificação Internacional do Tecido</th>
      <th>Fabricante</th>
      <th colspan="8"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogos.each do |catalogo| %>
      <tr>
        <td><%= catalogo.id %></td>
        <td><%= Suporte.find(catalogo.suporte_id).nome %></td>
        <td><%= catalogo.localizacao %></td>
        <td><%= catalogo.nome_aquisicao %></td>
        <td><%= catalogo.data_aquisicao %></td>
        <td><%= catalogo.fibra_tecido %></td>
        <td><%= catalogo.codificacao_internacional_tecido %></td>
        <td><%= catalogo.fabricante_id %></td>
        <td><%= link_to 'Show', catalogo %></td>
        <td><%= link_to 'Edit', edit_catalogo_path(catalogo) %></td>
        <td><%= link_to 'Destroy', catalogo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

