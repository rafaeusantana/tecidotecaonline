<h1>Lista de Tipos de Item Variado</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Tipo de Item Variado</th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_item_variados.each do |tipo_item_variado| %>
      <tr>
        <td><%= tipo_item_variado.tipo %></td>
        <td><%= link_to 'Mostrar', tipo_item_variado_path(tipo_item_variado) %></td>
        <td><%= link_to 'Editar', edit_tipo_item_variado_path(tipo_item_variado) %></td>
        <td><%= link_to 'Excluir', tipo_item_variado_path(tipo_item_variado), method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

