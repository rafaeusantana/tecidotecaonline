<h1>Lista de Fios Título</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Fio Título</th>
    </tr>
  </thead>

  <tbody>
    <% @fio_titulos.each do |fio_titulo| %>
      <tr>
        <td><%= fio_titulo.tipo %></td>
        <td><%= link_to 'Mostrar', fio_titulo_path(fio_titulo) %></td>
        <td><%= link_to 'Editar', edit_fio_titulo_path(fio_titulo) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Adicionar fio título', new_fio_titulo_path %>
