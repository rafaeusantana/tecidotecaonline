<h1>Lista de Texturas</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Tipo de Textura</th>
    </tr>
  </thead>

  <tbody>
    <% @texturas.each do |textura| %>
      <tr>
        <td><%= textura.tipo %></td>
        <td><%= link_to 'Mostrar', textura_path(textura) %></td>
        <td><%= link_to 'Editar', edit_textura_path(textura) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Adicionar textura', new_textura_path %>
